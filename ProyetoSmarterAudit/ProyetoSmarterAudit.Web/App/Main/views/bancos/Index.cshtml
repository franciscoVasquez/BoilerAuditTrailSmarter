<div ng-controller="app.views.banco.index as vm">
    <h1>@L("Bancos")</h1>
    <div class="row">
        <div class="col-md-12">
            @*<button ng-click="vm.openUserCreationModal()" class="btn btn-primary pull-right"><i class="fa fa-plus"></i> @L("CreateNewUser")</button>*@
            <table class="table">
                <thead>
                    <tr>
                        <th>@L("NroCuenta")</th>
                        <th>@L("Banco")</th>
                        <th>@L("TipoCuenta")</th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="banco in vm.bancos">
                        <td>{{banco.nroCuenta}}</td>
                        <td>{{banco.banco}}</td>
                        <td>{{banco.tipoCuenta}}</td>

                        @*<td>{{user.isActive ? '@L("Yes")' : '@L("No")'}}</td>*@
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
